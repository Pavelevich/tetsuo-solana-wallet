{
  "name": "tetsuo-solana-wallet",
  "version": "1.0.0",
  "description": "Secure CLI wallet for TETSUO on Solana with Grok AI assistant",
  "main": "dist/index.js",
  "bin": {
    "tetsuo": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "audit:deps": "npm audit --audit-level=moderate",
    "audit:verify": "node scripts/verify-integrity.js",
    "preinstall": "node scripts/preinstall-check.js || true"
  },
  "keywords": [
    "tetsuo",
    "solana",
    "wallet",
    "cli",
    "grok",
    "ai",
    "spl-token"
  ],
  "author": "TETSUO Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@solana/web3.js": "1.95.4",
    "@solana/spl-token": "0.4.9",
    "bs58": "6.0.0",
    "bip39": "3.1.0",
    "ed25519-hd-key": "1.3.0",
    "tweetnacl": "1.0.3",
    "chalk": "5.3.0",
    "inquirer": "9.3.7",
    "ora": "8.1.0",
    "boxen": "8.0.1",
    "figlet": "1.7.0",
    "gradient-string": "2.0.2",
    "commander": "12.1.0"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "@types/inquirer": "9.0.7",
    "@types/figlet": "1.5.8",
    "typescript": "5.6.3",
    "ts-node": "10.9.2",
    "jest": "29.7.0",
    "@types/jest": "29.5.14"
  },
  "overrides": {
    "protobufjs": "7.4.0"
  }
}
